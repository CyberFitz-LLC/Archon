+        
+    elif provider == "anthropic":
+        api_key = await credential_service.get_credential("ANTHROPIC_API_KEY")
+        config = {"api_key": api_key}
+        
+    return config
+
+def _convert_provider_status(status: ProviderStatus) -> ProviderHealthResponse:
